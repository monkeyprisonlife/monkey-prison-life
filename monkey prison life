local function PINNIW_fake_script() -- TextButton_11.Script 
	local script = Instance.new('Script', TextButton_11)

	script.Parent.MouseButton1Down:Connect(function()
		local Players = game:GetService("Players")
		local LocalPlayer = Players.LocalPlayer
	
		local function weldToPlayer(targetPlayer)
			local localHumanoidRoot = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
			local targetHumanoidRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
	
			if localHumanoidRoot and targetHumanoidRoot then
				local weld = Instance.new("WeldConstraint")
				weld.Part0 = localHumanoidRoot
				weld.Part1 = targetHumanoidRoot
				weld.Parent = localHumanoidRoot
				return weld
			end
		end
	
		local function fireMeleeEvent(targetPlayer, duration)
			local startTime = tick()
			while tick() - startTime < duration do
				game.ReplicatedStorage.meleeEvent:FireServer(targetPlayer)
				wait()
			end
		end
	
		local function teleportAndStickTo(player)
			if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
				local weld = weldToPlayer(player)
	
				fireMeleeEvent(player, 1.5)
	
				if weld then
					weld:Destroy()
				end
			end
		end
	
		local function teleportToAll()
			for _, player in pairs(Players:GetPlayers()) do
				if player ~= LocalPlayer then
					teleportAndStickTo(player)
				end
			end
		end
	
		teleportToAll()
	
		local hint = Instance.new("Hint", workspace)
		hint.Text = "Successfully killed all."
		wait(5)
		hint:Destroy()
	end)
end
coroutine.wrap(PINNIW_fake_script)()
local function HDXVIO_fake_script() -- TextButton_12.Script 
